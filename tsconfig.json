{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "lib": ["esnext"],
    "composite": false,
    "declaration": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true
  }
}
